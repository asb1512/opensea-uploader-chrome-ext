{
  "manifest_version": 3,

  "name": "Opensea Mass NFT Uploader",
  "description": "This extension aids in the mass-upload of NFTs.",
  "version": "1.0",

  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "default_locale": "en",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "contentSettings": ["<all_urls>"]
}